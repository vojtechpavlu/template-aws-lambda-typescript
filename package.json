{
  "name": "aws-lambda-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clear:deps": "rimraf ./node_modules",
    "clear:dist": "rimraf ./dist",
    "clear:artifact": "rimraf ./artifact && rimraf ./artifact.zip",
    "db:restart": "docker-compose -f ./database/docker-compose.local.yml down && docker-compose -f ./database/docker-compose.local.yml up --build",
    "compile": "npm run clear:dist && node compile.js",
    "package": "npm run clear:artifact && mkdir artifact && cp dist/* artifact/ && cd artifact && zip -r ../artifact.zip .",
    "format": "npx prettier . --write",
    "test:format": "npx prettier . --check",
    "test:lint": "eslint .",
    "test:repository": "jest --config=jest.repository.config.js",
    "test:repository:coverage": "jest --coverage --config=jest.repository.config.js --coverageDirectory=.coverage-repository",
    "test:resolver": "jest --config=jest.resolver.config.js",
    "test:resolver:coverage": "jest --coverage --config=jest.resolver.config.js --coverageDirectory=.coverage-resolver",
    "test:unit": "jest --config=jest.unit.config.js",
    "test:unit:coverage": "jest --coverage --config=jest.unit.config.js --coverageDirectory=.coverage-unit"
  },
  "author": "vojtechpavlu",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.148",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.0",
    "esbuild": "^0.25.4",
    "eslint": "^9.24.0",
    "eslint-plugin-depend": "^0.12.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.782.0",
    "@aws-sdk/lib-dynamodb": "^3.782.0",
    "dynamodb-toolbox": "^2.0.0",
    "zod": "^3.24.2"
  }
}
